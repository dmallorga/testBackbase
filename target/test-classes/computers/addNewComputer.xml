<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Add New Computer Test Suit" verbose="1" parallel="false">
  <test name="Add New Computer Scripts">
    <classes>
      <class name="tests.AddNewComputerTests">
        <parameter name="computerName" value="Daniel's Computer"></parameter>
        <parameter name="introducedDate" value="2018-05-29"></parameter>
        <parameter name="discontinuedDate" value="2018-12-24"></parameter>
        <parameter name="companyName" value="Tandy Corporation"></parameter>
        <methods>
          <include name="addNewComputerScreenDisplays"/>
          <include name="addNewComputerByOnlyEnteringRequiredFields"/>
          <include name="cannotAddNewComputerWithoutEnteringRequiredField"/>
        </methods>
      </class>
    </classes>
  </test>
</suite>
